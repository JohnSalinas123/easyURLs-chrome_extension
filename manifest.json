{
    "name": "easyURLs",
    "description": "Copy URLs of multiple tabs easily!",
    "version": "1.0",
    "manifest_version": 3,
    "background" : {
        "service_worker": "background.js"
    },
    "permissions": ["storage","activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icons": {
            
        }
    },
    "icons": {
        
    }

}