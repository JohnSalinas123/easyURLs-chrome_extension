{
    "name": "easyURLs",
    "description": "Copy URLs of multiple tabs easily!",
    "version": "1.0",
    "manifest_version": 3,
    "background" : {
        "service_worker": "popup.js"
    },
    "permissions": [
        "storage","activeTab", "scripting", "tabs"
    ],
    "action": {
        "default_popup": "popup.html"
    }

}